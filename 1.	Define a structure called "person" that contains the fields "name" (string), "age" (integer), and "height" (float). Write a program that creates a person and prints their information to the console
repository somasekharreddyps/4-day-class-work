#include <stdio.h>

struct person {
    char name[50];
    int age;
    float height;
};

int main() {
    // Create a person
    struct person p;
    strcpy(p.name, "John");
    p.age = 25;
    p.height = 1.75;

    // Print person's information
    printf("Name: %s\n", p.name);
    printf("Age: %d\n", p.age);
    printf("Height: %.2f\n", p.height);

    return 0;
}
